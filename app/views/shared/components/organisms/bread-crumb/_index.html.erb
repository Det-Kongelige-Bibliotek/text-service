<div class="org-01c45fc6-df1a-4fd8-b8c3-d1ff8e3b519d <%= classes %> show-for-large">
  <div class="inner-content grid-container">
    <div class="grid-x grid-margin-x">
      <div class="hide-for-small-only cell small-12">
        <div class="bread-crumb">
          <%= render :partial =>'shared/components/atoms/icon/index', locals: {icon_type: 'home', icon_size: 'icon-xsmall'}%>
          <a href="http://www.kb.dk">Forside</a>
          <% items.each_with_index do | item, index |%>
              / <% if (index == items.size - 1) | (item[1] == '') %>
                  <%= item[0]%>
                <% else %>
                  <a href="<%= item[1] %>"><%= item[0]%></a>
                <% end %>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
